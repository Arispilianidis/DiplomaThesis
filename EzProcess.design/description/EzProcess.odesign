<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/description/tool/1.1.0" xmlns:tool_1="http://www.eclipse.org/sirius/diagram/description/tool/1.1.0" xmlns:validation="http://www.eclipse.org/sirius/description/validation/1.1.0" name="EzProcess" version="12.0.0.2017041100">
  <ownedViewpoints name="App" modelFileExtension="ezprocess">
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="App Diagram" domainClass="ezProcess::App">
      <metamodel href="http://www.example.org/ezProcess#/"/>
      <validationSet>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="QuestionOnlyInScreening" level="ERROR" message="You can only use Questions when you are working on a Screening Process" targetClass="ezProcess::Question">
          <audits auditExpression="aql: self.eContainer(ezProcess::Process).oclIsTypeOf(ezProcess::Screening)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="RatingOnlyInScreening" level="ERROR" message="You can only use Rating when you are working on a Screening Process" targetClass="ezProcess::Rating">
          <audits auditExpression="aql: self.eContainer(ezProcess::Process).oclIsTypeOf(ezProcess::Screening)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="OptionRatingOnlyInJobDescription" level="ERROR" message="You can only use OptionRating when you are working on a Job Description Process" targetClass="ezProcess::OptionRating">
          <audits auditExpression="aql: self.eContainer(ezProcess::Process).oclIsTypeOf(ezProcess::JobDescription)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="NoCandidateTextFieldsInJobDescription" level="ERROR" message="You can not use Candidate Text Field when you are working on a Job Description Process." targetClass="ezProcess::CandidateTextField">
          <audits auditExpression="aql: not self.eContainer(ezProcess::Process).oclIsTypeOf(ezProcess::JobDescription)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="NoCandidateEmailFieldsInJobDescription" level="ERROR" message="You can not use Candidate Email Field when you are working on a Job Description Process." targetClass="ezProcess::CandidateEmailField">
          <audits auditExpression="aql: not self.eContainer(ezProcess::Process).oclIsTypeOf(ezProcess::JobDescription)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="NoRatingOnCandidateTextFields" level="ERROR" message="You can not use Rating on a Candidate Text Field" targetClass="ezProcess::CandidateTextField">
          <audits auditExpression="aql: self.hasRating->size()=0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="NoRatingOnCandidateEmailFields" level="ERROR" message="You can not use Rating on a Candidate EmailField" targetClass="ezProcess::CandidateEmailField">
          <audits auditExpression="aql: self.hasRating->size()=0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="FreeTextRating" level="ERROR" message="If rating=true in a Free Text Area then you need to assign a Rating component." targetClass="ezProcess::FreeTextArea">
          <audits auditExpression="service: freeTextRating()"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="InputFieldRating" level="ERROR" message="If rating=true in an Input Field then you need to assign a Rating component." targetClass="ezProcess::InputField">
          <audits auditExpression="service: freeInputRating()"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="OnboardingTrainingTasksOnlyInOnboarding" level="ERROR" message="You can only use Onboarding Training Task when you are working on an Onboarding Process" targetClass="ezProcess::OnboardingTrainingTask">
          <audits auditExpression="aql: self.eContainer(ezProcess::Process).oclIsTypeOf(ezProcess::Onboarding)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="OnboardingSetupTasksOnlyInOnboarding" level="ERROR" message="You can only use Onboarding Setup Task when you are working on an Onboarding Process" targetClass="ezProcess::OnboardingSetupTask">
          <audits auditExpression="aql: self.eContainer(ezProcess::Process).oclIsTypeOf(ezProcess::Onboarding)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="NoSpaceAtProcessNames" level="ERROR" message="The name property of a Process should not contain empty spaces." targetClass="ezProcess::Process">
          <audits auditExpression="aql: not( self.name.trim().contains(' '))"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="NoSpaceAtValueFieldNames" level="ERROR" message="The name property of a Value Field should not contain empty spaces." targetClass="ezProcess::InitialValue">
          <audits auditExpression="aql: not( self.name.trim().contains(' '))"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="NoSpaceAtOnboardingTaskNames" level="ERROR" message="The name property of a Value Field should not contain empty spaces." targetClass="ezProcess::OnboardingTask">
          <audits auditExpression="aql: not( self.name.trim().contains(' '))"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="UniqueProcessNameValues" level="ERROR" message="The name property of a Value Field should be unique." targetClass="ezProcess::InitialValue">
          <audits auditExpression="aql: self.eContainer(ezProcess::Process).eAllContents()->filter(ezProcess::InitialValue)->select(v | v.name = self.name)->size() = 1"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="UniqueOptionVarNameValues" level="ERROR" message="The optionVarName property of a Value Field should be unique." targetClass="ezProcess::MultipleOptions">
          <audits auditExpression="aql: self.eContainer(ezProcess::Process).eAllContents()->filter(ezProcess::MultipleOptions)->select(v | v.optionVarName = self.optionVarName)->size() = 1"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="NoSpaceAtOptionVarNameFields" level="ERROR" message="The Option Var Name property of a Value Field should not contain empty spaces." targetClass="ezProcess::MultipleOptions">
          <audits auditExpression="aql: not( self.name.trim().contains(' '))"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="OnlyOneFirstPageAtEachProcess" level="ERROR" message="There must be one Page per Process which has first property equal to true." targetClass="ezProcess::Page">
          <audits auditExpression="aql: self.eContainer(ezProcess::Process).eAllContents(ezProcess::Page)->select(p | p.first)->size()=1"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="OnlyOneFinalPageAtEachProcess" level="ERROR" message="There must be one Page per Process which has final property equal to true." targetClass="ezProcess::Page">
          <audits auditExpression="aql: self.eContainer(ezProcess::Process).eAllContents(ezProcess::Page)->select(p | p.final)->size()=1"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="NoFirstAndFinal" level="ERROR" message="There can not be a Page with the properties first and final equal to true at the same time." targetClass="ezProcess::Page">
          <audits auditExpression="aql: self.eContainer(ezProcess::Process).eAllContents(ezProcess::Page)->select(p | p.final and p.first)->size()= 0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="UniqueEmployeeIDs" level="ERROR" message="The id property of an Employee should be unique." targetClass="ezProcess::Employee">
          <audits auditExpression="aql: self.eContainer(ezProcess::App).eAllContents()->filter(ezProcess::Employee)->select(v | v.id = self.id)->size() = 1"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="UniqueQuestionIDs" level="ERROR" message="The ID property of a Question should be unique." targetClass="ezProcess::Question">
          <audits auditExpression="aql: self.eContainer(ezProcess::Process).eAllContents()->filter(ezProcess::Question)->select(v | v.ID= self.ID)->size() = 1"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="UniqueOptionRatingIDs" level="ERROR" message="The id property of an Option Rating should be unique." targetClass="ezProcess::OptionRating">
          <audits auditExpression="aql: self.eContainer(ezProcess::Process).eAllContents()->filter(ezProcess::OptionRating)->select(v | v.ID= self.ID)->size() = 1"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="PositiveEmployeeIDs" level="ERROR" message="The id property of an Employee should be positive." targetClass="ezProcess::Employee">
          <audits auditExpression="aql: self.id >= 0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="PositiveQuestionIDs" level="ERROR" message="The ID property of an Question should be positive." targetClass="ezProcess::Question">
          <audits auditExpression="aql: self.ID >= 0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="PositiveOptionRatingIDs" level="ERROR" message="The ID property of an Option Rating should be positive." targetClass="ezProcess::OptionRating">
          <audits auditExpression="aql: self.ID >= 0"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="QuestionIsAboutValueFromJobDescription" level="ERROR" message="The isAbout property of a Question must refer to a Value Field from a Job Description Process." targetClass="ezProcess::Question">
          <audits auditExpression="aql: self.isAbout.eContainer(ezProcess::Process).oclIsTypeOf(ezProcess::JobDescription)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="OnboardingSetupIsAboutValueFromJobDescription" level="ERROR" message="The isAbout property of an Onboarding Setup Task must refer to a Value Field from a Job Description Process." targetClass="ezProcess::OnboardingSetupTask">
          <audits auditExpression="aql: self.isAbout.eContainer(ezProcess::Process).oclIsTypeOf(ezProcess::JobDescription)"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="UniqueQuestionAtOptionRatingAtSameOption" level="ERROR" message="The property isAbout of Option Rating components, at the same Option component, should not refer to the same Question ID." targetClass="ezProcess::OptionRating">
          <audits auditExpression="aql: self.eContainer().eAllContents(ezProcess::OptionRating)->select(o | o.isAbout = self.isAbout)->size()=1"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="OptionRatingSizeEqualsOptionSize" level="ERROR" message="If a Question refers to a Value Field, then each option of the Value Field should contain an Option Rating about that Question." targetClass="ezProcess::Question">
          <audits auditExpression="aql: self.isAbout.hasOption.hasOptionRating->select(o | o.isAbout.ID=self.ID)->size() = self.isAbout.hasOption->size()"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="FirstPageOfScreeningContainsCandidateInfo" level="ERROR" message="The first page of the Screening process should contain all Candidate Info (First Name, Last Name and Email)" targetClass="ezProcess::Screening">
          <audits auditExpression="aql: self.hasPage->select(p | p.first).hasInitialvalue->select(v | v.oclIsKindOf(ezProcess::CandidateTextField)).candidateField.toString()->includesAll(Sequence{'FIRST_NAME','LAST_NAME')}) and self.hasPage->select(p | p.first).hasInitialvalue->select(v | v.oclIsKindOf(ezProcess::CandidateEmailField))->size()= 1"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="FirstPageOfOnBoardingContainsCandidateInfo" level="ERROR" message="The first page of the Onboarding process should contain all Candidate Info (First Name, Last Name and Email)" targetClass="ezProcess::Onboarding">
          <audits auditExpression="aql: self.hasPage->select(p | p.first).hasInitialvalue->select(v | v.oclIsKindOf(ezProcess::CandidateTextField)).candidateField.toString()->includesAll(Sequence{'FIRST_NAME','LAST_NAME')}) and self.hasPage->select(p | p.first).hasInitialvalue->select(v | v.oclIsKindOf(ezProcess::CandidateEmailField))-> size()= 1"/>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="WarningHasRatingWithoutPropertyRatingTrue" level="WARNING" message="You assigned a Rating component but the rating property of the value is false." targetClass="ezProcess::InitialValue">
          <audits auditExpression="aql: if self.rating=false and self.hasRating->size()>0 then false else true endif"/>
          <fixes name="RatingTrue">
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="var:self">
                <subModelOperations xsi:type="tool:SetValue" featureName="rating" valueExpression="true"/>
              </firstModelOperations>
            </initialOperation>
          </fixes>
        </ownedRules>
        <ownedRules xsi:type="validation:SemanticValidationRule" name="UniqueOnboardingTaskNames" level="ERROR" message="The name property of an OnboardingTask should be unique." targetClass="ezProcess::OnboardingTask">
          <audits auditExpression="aql: self.eContainer(ezProcess::Process).eAllContents()->filter(ezProcess::OnboardingTask)->select(v | v.name = self.name)->size() = 1"/>
        </ownedRules>
      </validationSet>
      <defaultLayer name="Default">
        <nodeMappings name="EmployeeNode" label="Employee" semanticCandidatesExpression="feature:hasEmployee" domainClass="ezProcess::Employee">
          <style xsi:type="style:WorkspaceImageDescription" labelSize="12" showIcon="false" labelExpression="aql: self.firstName + ' ' + self.lastName" sizeComputationExpression="5" resizeKind="NSEW" workspacePath="/EzProcess.design/icons/Person.jpg">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="OnboardingHasScreeningEdge" sourceMapping="//@ownedViewpoints[name='App']/@ownedRepresentations[name='App%20Diagram']/@defaultLayer/@containerMappings[name='OnboardingContainer']" targetMapping="//@ownedViewpoints[name='App']/@ownedRepresentations[name='App%20Diagram']/@defaultLayer/@containerMappings[name='ScreeningContainer']" targetFinderExpression="aql: self.hasScreening">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_orange']"/>
            <centerLabelStyleDescription labelSize="12" labelExpression="hasScreening">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="ScreeningHasJobDescdge" sourceMapping="//@ownedViewpoints[name='App']/@ownedRepresentations[name='App%20Diagram']/@defaultLayer/@containerMappings[name='ScreeningContainer']" targetMapping="//@ownedViewpoints[name='App']/@ownedRepresentations[name='App%20Diagram']/@defaultLayer/@containerMappings[name='JobDescriptionContainer']" targetFinderExpression="aql: self.hasJobDescription">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='light_orange']"/>
            <centerLabelStyleDescription labelSize="12" labelExpression="hasJobDescription">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <edgeMappings name="EmployeeHasProcessEdge" sourceMapping="//@ownedViewpoints[name='App']/@ownedRepresentations[name='App%20Diagram']/@defaultLayer/@nodeMappings[name='EmployeeNode']" targetMapping="//@ownedViewpoints[name='App']/@ownedRepresentations[name='App%20Diagram']/@defaultLayer/@containerMappings[name='ScreeningContainer'] //@ownedViewpoints[name='App']/@ownedRepresentations[name='App%20Diagram']/@defaultLayer/@containerMappings[name='OnboardingContainer'] //@ownedViewpoints[name='App']/@ownedRepresentations[name='App%20Diagram']/@defaultLayer/@containerMappings[name='JobDescriptionContainer']" targetFinderExpression="aql: self.assignedProcess">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='green']"/>
